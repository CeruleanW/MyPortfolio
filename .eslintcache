ngCount":0,"usedDeprecatedRules":"110"},{"filePath":"208","messages":"209","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"110"},{"filePath":"210","messages":"211","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"110"},{"filePath":"212","messages":"213","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"110"},{"filePath":"214","messages":"215","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"110"},{"filePath":"216","messages":"217","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"110"},{"filePath":"218","messages":"219","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"110"},{"filePath":"220","messages":"221","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"110"},"/home/yi/Code/MyPortfolio/src/index.js",[],["222","223"],"/home/yi/Code/MyPortfolio/src/App.js",["224","225"],"import React, { lazy, useEffect, useState, useRef } from 'react';\nimport Footer from './components/Footer/Footer';\nimport Nav from './components/organisms/Nav';\nimport NavTabs from './components/molecules/NavTabs';\nimport { BrowserRouter, Switch, Route } from 'react-router-dom';\nimport { AnimatePresence } from 'framer-motion';\nimport Theme from './styles/base/Theme';\nimport CssBaseline from '@material-ui/core/CssBaseline';\nimport ScrollToTop from './components/atomics/ScrollToTop';\nimport {\n  STATEMENT,\n  PAGE_ROUTES,\n  PAGE_TITLES,\n  PERSONAL_DATA_LINK,\n} from './data/globals';\nimport './styles/main.scss';\nimport axios from 'axios';\nimport yaml from 'js-yaml';\nimport LogRocket from 'logrocket';\nimport {easterEgg} from './lib/easter-egg';\nimport { LinearProgress } from '@material-ui/core';\n\nLogRocket.init('5bsway/portfolio');\n\nconst Home = lazy(() => import('./pages/Home'));\nconst Projects = lazy(() => import('./pages/Projects'));\nconst AboutMe = lazy(() => import('./pages/AboutMe'));\nconst Contact = lazy(() => import('./pages/Contact'));\nconst ProjectDetailPage = lazy(() => import('./pages/ProjectDetailPage'));\nconst Resume = lazy(() => import('./pages/Resume'));\n\nexport default function App() {\n  const [doc, setDoc] = useState(null);\n  const resumeData = useRef(null);\n  const resumeVersion = useRef('frontend');\n\n\n  useEffect(() => {\n    async function foo() {\n      const res = await axios.get(PERSONAL_DATA_LINK);\n      setDoc(yaml.load(res.data));\n    }\n    foo();\n\n    easterEgg();\n    console.log(STATEMENT);\n  }, []);\n\n  return (\n    <Theme>\n      <CssBaseline />\n      <BrowserRouter>\n        <div className={'root-container'}>\n          <Nav routes={PAGE_ROUTES} pageTitles={PAGE_TITLES}>\n            <Route\n              path='/'\n              render={({ location }) => (\n                <NavTabs\n                  routes={PAGE_ROUTES}\n                  value={location.pathname}\n                  pageTitles={PAGE_TITLES}\n                />\n              )}\n            />\n          </Nav>\n          <React.Suspense fallback={<LinearProgress color={'secondary'}/>}>\n            <main className={'root-main'}>\n              <Route\n                render={({ location }) => (\n                  <AnimatePresence exitBeforeEnter initial={false}>\n                    <Switch key={location.pathname}>\n                      <Route\n                        exact\n                        path={PAGE_ROUTES[0]}\n                        render={() => <Home />}\n                      />\n                      <Route\n                        path={`${PAGE_ROUTES[1]}/:id`}\n                        render={() => <ProjectDetailPage />}\n                      />\n                      <Route\n                        path={PAGE_ROUTES[1]}\n                        render={() => <Projects />}\n                      />\n                      <Route path={PAGE_ROUTES[2]} render={() => <AboutMe />} />\n                      <Route path={PAGE_ROUTES[3]} render={() => <Contact />} />\n                      {doc ? (\n                        <Route\n                          path={PAGE_ROUTES[4]}\n                          render={() => (\n                            <Resume {...doc.names} {...doc.resume} />\n                          )}\n                        />\n                      ) : null}\n                    </Switch>\n                  </AnimatePresence>\n                )}\n              />\n            </main>\n            <Footer />\n            <ScrollToTop />\n          </React.Suspense>\n        </div>\n      </BrowserRouter>\n    </Theme>\n  );\n}\n","/home/yi/Code/MyPortfolio/src/reportWebVitals.js",[],"/home/yi/Code/MyPortfolio/src/data/globals.js",[],"/home/yi/Code/MyPortfolio/src/lib/easter-egg.js",[],"/home/yi/Code/MyPortfolio/src/styles/base/Theme.jsx",[],"/home/yi/Code/MyPortfolio/src/pages/Resume.jsx",[],"/home/yi/Code/MyPortfolio/src/pages/Home.jsx",[],"/home/yi/Code/MyPortfolio/src/pages/Projects.jsx",["226"],"import PropTypes from 'prop-types';\nimport React from 'react';\nimport ProjectSection from '../components/organisms/ProjectSection';\nimport data from '../data/projects.json';\nimport styles from '../styles/pages/Projects.module.scss';\n\nconst projectData = data.projects;\n\nexport default function Projects() {\n  return (\n    <>\n      <h1 className={`text-center text-5xl p-5 pb-0 w-full lg:mt-5 ${styles.textshadow}`}>\n        Projects\n      </h1>\n      {projectData.map((project, index) => {\n        const isRightNarrow = index % 2 !== 0;\n        return (\n          <ProjectSection\n            key={project.id}\n            isRightNarrow={isRightNarrow}\n            index={index}\n            className={styles.section}\n            {...project}\n          ></ProjectSection>\n        );\n      })}\n    </>\n  );\n}\n","/home/yi/Code/MyPortfolio/src/pages/Contact.jsx",[],"/home/yi/Code/MyPortfolio/src/pages/AboutMe.jsx",[],"/home/yi/Code/MyPortfolio/src/pages/ProjectDetailPage.jsx",["227"],"import React from 'react';\nimport { Typography, Box, Grid, Container } from '@material-ui/core';\nimport Description from '../components/molecules/Description';\nimport { useParams } from 'react-router-dom';\nimport { PropTypes } from 'prop-types';\nimport data from '../data/projects.json';\nimport { FeatureList } from '../components/MetaDataList';\n\n//TODO:\n// pics for projects\nconst { projects } = data;\n\nfunction Title(props) {\n  return (\n    <>\n      <Typography variant='h1'>{props.children}</Typography>\n    </>\n  );\n}\n\nexport function MetaData(props) {\n  return (\n    <Grid container justify={'space-between'}>\n      {props.children}\n    </Grid>\n  );\n}\n\nfunction getProject(targetId) {\n  const result = projects.find(({ id }) => id === targetId);\n  return result;\n}\n\nfunction Heading(props) {\n  return (\n    <div>\n      <Typography variant={'h2'}>{props.children}</Typography>\n    </div>\n  );\n}\n\nexport default function ProjectDetailPage(props) {\n  const { id } = useParams(); //pure item id\n  \n  //use the project id to find project data\n  const projectData = getProject(id);\n\n  const {\n    title,\n    content,\n    live,\n    type,\n    techs,\n    repo,\n    illustrations,\n  } = projectData;\n\n  // render the img by index\n  const renderIllustration = (index) => {\n    try {\n      if (illustrations[index]) {\n        return (\n          <Box\n            maxWidth={1000}\n            maxHeigth={1280}\n            display='flex'\n            justifyContent='center'\n          >\n            <img\n              src={illustrations[index]}\n              alt={'illustration'}\n              style={{ maxWidth: ' 100%', height: 'auto' }}\n            />\n          </Box>\n        );\n      }\n    } catch {\n      return null;\n    }\n  };\n\n  return (\n    <>\n      <Container>\n        <Box mt={14}>\n          <Title>{title}</Title>\n          <Box mt={6} maxWidth={800}>\n            <Description>{content.introduction}</Description>\n          </Box>\n          <Box mt={4} maxWidth={870}>\n            <MetaData>\n              <FeatureList label={'Type'} item={type} />\n              <FeatureList label={'Techs'} items={techs} />\n              <FeatureList\n                label={'Source Code'}\n                item={repo}\n                isLink={true}\n                linktext={'Repo'}\n              />\n              <FeatureList\n                label={'Demo'}\n                item={live}\n                isLink={true}\n                linktext={'Visit Site'}\n              />\n            </MetaData>\n          </Box>\n        </Box>\n        <Box mt={4}>{renderIllustration(0)}</Box>\n        <Box mt={6} maxWidth={1024}>\n          <Heading>Project Goal</Heading>\n          <Box mt={2}>\n            <Description>{content.goal}</Description>\n          </Box>\n        </Box>\n        {content.spotlight ? (\n          <Box mt={8} maxWidth={1024}>\n            <Heading>Spotlight</Heading>\n            <Box mt={2}><Description>{content.spotlight}</Description></Box>\n          </Box>\n        ) : null}\n        <Box mt={10}>\n          <Grid container spacing={5}>\n            <Grid item xs={12} lg={6}>\n              {renderIllustration(1)}\n            </Grid>\n            <Grid item xs={12} lg={6}>\n              <Heading>Problems</Heading>\n              <Box mt={5} maxWidth={650}>\n                {content.problem.map((p, index) => (\n                  <Description key={'para-' + index}>{p}</Description>\n                ))}\n              </Box>\n            </Grid>\n          </Grid>\n        </Box>\n        <Box mx={'auto'} mt={10} mb={10} maxWidth={1024}>\n          <Typography variant={'h2'} align={'center'}>\n            Lessons Learned\n          </Typography>\n          <Box mt={6}>\n            {content.lesson.map((p, index) => (\n              <Description key={'para-' + index} align={'center'}>\n                {p}\n              </Description>\n            ))}\n          </Box>\n          <Box mt={4}>{renderIllustration(2)}</Box>\n        </Box>\n      </Container>\n    </>\n  );\n}\n","/home/yi/Code/MyPortfolio/src/components/Footer/Footer.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/ResumeSectionWrapper.js",[],"/home/yi/Code/MyPortfolio/src/components/MetaDataList.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/ContactItem.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/Duration.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/Features.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/Language.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/SkillsSection.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Footer/FooterContent.jsx",[],"/home/yi/Code/MyPortfolio/src/components/molecules/CustomCard.jsx",[],"/home/yi/Code/MyPortfolio/src/components/atomics/DotBox.jsx",[],"/home/yi/Code/MyPortfolio/src/components/atomics/Heading.jsx",[],"/home/yi/Code/MyPortfolio/src/components/organisms/Nav.jsx",[],"/home/yi/Code/MyPortfolio/src/components/organisms/MyStory.jsx",[],"/home/yi/Code/MyPortfolio/src/components/organisms/MySkills.jsx",["228"],"import React from 'react';\nimport Heading from '../atomics/Heading';\nimport {\n  Typography,\n  ListItem,\n} from '@material-ui/core';\nimport styles from '../../styles/components/components.module.scss';\n\nconst SkillSet = (props) => {\n  const { title } = props;\n\n  return (\n    <div className='mb-4'>\n      <p className={`${styles['skill-header']} mb-3`}>{title}</p>\n      <div className='w-full flex pl-4 flex-wrap'>{props.children}</div>\n    </div>\n  );\n};\n\nconst SimpleSkill = (props) => {\n  return (\n    <ListItem>\n      <Typography>\n        <span style={{ color: 'cadetblue', marginRight: '6px' }}>●</span>\n        {props.children}\n      </Typography>\n    </ListItem>\n  );\n};\n\nconst Skill = (props) => {\n  return <li className={`${styles['skill-item']}`}>{props.children}</li>;\n};\n\nconst ExpandableSkill = (props) => {\n  return <SimpleSkill>{props.children}</SimpleSkill>;\n};\n\nexport default function MySkills() {\n  return (\n    <>\n      <Heading>My Skills</Heading>\n      <div className='flex flex-col justify-between lg:flex-row mt-8'>\n        <SkillSet title={'Coding Language'}>\n          <Skill>JavaScript</Skill>\n          <Skill>TypeScript</Skill>\n          <Skill>Java</Skill>\n          <Skill>HTML5</Skill>\n          <Skill>CSS3</Skill>\n          <Skill>Sass/Scss</Skill>\n          <Skill>SQL</Skill>\n          <Skill>Python</Skill>\n        </SkillSet>\n        <SkillSet title={'Frameworks & Libs'}>\n          <Skill>React.js</Skill>\n          <Skill>Redux</Skill>\n          <Skill>React Native</Skill>\n          <Skill>Next.js</Skill>\n          <Skill>TailwindCSS</Skill>\n          <Skill>Material UI</Skill>\n          <Skill>Jest, JUnit</Skill>\n        </SkillSet>\n        <SkillSet title={'Methods & Tools'}>\n          <Skill>Agile Methodology</Skill>\n          <Skill>Webpack</Skill>\n          <Skill>Babel</Skill>\n          <Skill>MongoDB</Skill>\n          <Skill>MatLab</Skill>\n          <Skill>R</Skill>\n          <Skill>Test-Driven Development</Skill>\n        </SkillSet>\n        <SkillSet title={'Additional'}>\n          <Skill>UML</Skill>\n          <Skill>SAP</Skill>\n          <Skill>Bizagi BPMN Modeler</Skill>\n        </SkillSet>\n      </div>\n    </>\n  );\n}\n","/home/yi/Code/MyPortfolio/src/components/molecules/Description.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/sections/HeaderSection.js",[],"/home/yi/Code/MyPortfolio/src/components/molecules/More.jsx",[],"/home/yi/Code/MyPortfolio/src/components/atomics/ScrollToTop.jsx",[],"/home/yi/Code/MyPortfolio/src/components/atomics/SVGLine.jsx",[],"/home/yi/Code/MyPortfolio/src/components/organisms/ProjectSection.jsx",["229"],"import PropTypes from 'prop-types';\nimport React from 'react';\nimport { Button } from '@material-ui/core';\nimport { motion } from 'framer-motion';\nimport { Link, useRouteMatch } from 'react-router-dom';\nimport styles from '../../styles/components/components.module.scss';\n\n// Single project section\n\nconst scaleAni = { whileHover: { scale: 1.1 }, whileTap: { scale: 0.9 } };\n\nconst ProjectCard = (props) => {\n  const { id, title, image } = props;\n  const { url } = useRouteMatch();\n\n  return (\n    <motion.div className={`w-11/12 lg:w-2/5 max-w-full mt-4`} {...scaleAni}>\n      <Link to={`${url}/${id}`}>\n        <img src={image} alt={title} className={styles['img-fit']} />\n      </Link>\n    </motion.div>\n  );\n};\n\nconst ProjectText = (props) => {\n  const { isRightNarrow, title, subtitle, id, index } = props;\n  const { url } = useRouteMatch();\n\n  return (\n    <div\n      className={`text-center max-w-full mt-8 lg:max-w-screen-sm  ${\n        isRightNarrow ? 'lg:ml-14' : 'order-first lg:mr-14'\n      }`}\n    >\n      <motion.div {...scaleAni}>\n        <h2 className={`text-center text-2xl lg:text-left`}>\n          {`${index + 1}. ${title}`}\n        </h2>\n        <p className={`mt-3 text-base lg:text-left`}>{subtitle}</p>\n        <div\n          className={`flex flex-wrap mt-20 lg:mt-14 justify-center lg:justify-start`}\n        >\n          {props.techs.map((tech, index) => (\n            <span\n              key={'tech-' + tech}\n              className={`${styles.chip} ${index === 0 ? 'pl-0 pr-2' : 'px-2'}`}\n            >\n              {tech}\n            </span>\n          ))}\n        </div>\n      </motion.div>\n      <motion.div\n        className={`flex justify-center items-center mt-8 mb-6`}\n        {...scaleAni}\n      >\n        <Button\n          variant='contained'\n          color='primary'\n          size='large'\n          component={Link}\n          to={`${url}/${id}`}\n        >\n          Check it out!\n        </Button>\n      </motion.div>\n    </div>\n  );\n};\n\nexport default function ProjectSection(props) {\n  // Structure:\n  // - title\n  // - function\n  // - explanations of projects to illustrate how you handle problems\n\n  return (\n    <section className={`${props.className} `}>\n      <div className='flex flex-col lg:flex-row justify-center items-center mt-10 lg:mt-4 max-w-full flex-grow'>\n        <ProjectCard {...props} />\n        <ProjectText {...props} />\n      </div>\n    </section>\n  );\n}\n","/home/yi/Code/MyPortfolio/src/components/organisms/Menu.jsx",[],"/home/yi/Code/MyPortfolio/src/components/molecules/NavTabs.jsx",[],"/home/yi/Code/MyPortfolio/src/components/organisms/ProjectsCarousel/index.jsx",[],"/home/yi/Code/MyPortfolio/src/components/molecules/SocialMedia.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/sections/ExperienceSection.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/sections/EducationSection.jsx",["230"],"import React from 'react';\nimport Section from '../ResumeSectionWrapper';\nimport Title from '../atomics/Title';\nimport Duration from '../Duration';\nimport Location from '../atomics/Location';\nimport SchoolName from '../atomics/SchoolName';\nimport Features from '../Features';\n\nexport function EducationSection(props) {\n  const { education } = props;\n\n  function renderItem(education) {\n    const {\n      major, degree, organization, from, to, location, statements, gpa,\n    } = education;\n    const projects = education['school-projects'];\n\n    return (\n      <div className='inner-section' key={`${organization}-${major}`}>\n        <h3 className='text-2xl'>\n          <span className='text-2xl'>{major}</span>,{' '}\n          <span className='text-2xl'>{degree}</span>\n        </h3>\n        <SchoolName>{organization}</SchoolName>\n        <Duration {...{ from, to }} />\n        <span className='mr-20'></span>\n        <Location>{location}</Location>\n        {!!statements ? <Features features={statements} /> : null}\n        {!!projects ? <Features features={projects.features} /> : null}\n        {/* {!!gpa ? <GPA>{gpa}</GPA> : null} */}\n      </div>\n    );\n  }\n\n  return (\n    <Section id='education'>\n      <Title text='education' />\n      {Array.isArray(education)\n        ? education.map((item) => {\n          return renderItem(item);\n        })\n        : renderItem(education)}\n    </Section>\n  );\n}\n","/home/yi/Code/MyPortfolio/src/components/Resume/sections/ProjectsSection.jsx",["231"],"import React from 'react';\nimport Section from '../ResumeSectionWrapper';\nimport Title from '../atomics/Title';\nimport ProjectName from '../atomics/ProjectName';\nimport ProjectLink from '../atomics/ProjectLink';\nimport Features from '../Features';\n\nexport function ProjectsSection(props) {\n  // Content for the project section\n  return (\n    <Section id='projects'>\n      <Title text='Side projects' />\n      {props.projects.map((project) => {\n        const { features, repo, demo, title } = project;\n        return (\n          <div key={title} className='inner-section'>\n            <ProjectName text={title} />\n            <ProjectLink link={demo} />\n            <Features features={features}></Features>\n          </div>\n        );\n      })}\n    </Section>\n  );\n}\n","/home/yi/Code/MyPortfolio/src/components/Resume/atomics/Title.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/Content.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/SkillItem.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/SchoolName.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/Location.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/Spotlight.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/CompanyIntro.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/CompanyName.jsx",[],"/home/yi/Code/MyPortfolio/src/components/Resume/atomics/Positi