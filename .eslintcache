 = useRouteMatch();\n\n  return (\n    <Box maxWidth={500}>\n      <motion.div {...scaleAni}>\n      <Box mt={4}>\n        <Typography\n          variant={\"h5\"}\n          component={\"p\"}\n          className={classes.title}\n          align={isRightNarrow ? \"left\" : \"right\"}\n        >\n          {title}\n        </Typography>\n        <Typography align={isRightNarrow ? \"left\" : \"right\"}>\n          {subtitle}\n        </Typography>\n      </Box>\n      <Box\n        className={classes.techContainer}\n        justifyContent={isRightNarrow ? \"flex-start\" : \"flex-end\"}\n      >\n        {props.techs.map((tech, index) => (\n          <Chip key={\"tech-\" + index} label={tech} className={classes.tech} />\n        ))}\n      </Box>\n      </motion.div>\n      <Box\n        display=\"flex\"\n        justifyContent={isRightNarrow ? \"flex-start\" : \"flex-end\"}\n        alignItems={\"center\"}\n        mt={8}\n      >\n        <motion.div {...scaleAni}>\n          <Button\n            variant=\"contained\"\n            color=\"primary\"\n            size=\"large\"\n            component={Link} to={`${url}/${id}`}\n          >\n            Check it out!\n          </Button>\n        </motion.div>\n      </Box>\n    </Box>\n  );\n};\n\nexport default function ProjectSection(props) {\n  const classes = useStyles();\n  const { image, isRightNarrow} = props;\n\n  const [isHover, setIsHover] = useState(false);\n  // const [isLoaded, setIsLoaded] = useState(false);\n  const imagePath = process.env.PUBLIC_URL + \"/img/\" + image; // local image path\n\n  return (\n    <>\n      <section>\n        <Grid\n          container\n          justify={\"center\"}\n          alignItems={\"center\"}\n          className={classes.gridContainer}\n        >\n          {isRightNarrow ? (\n            <Grid item lg={6} xs={11}>\n              <ProjectCard\n                onAppear={true}\n                imagePath={imagePath}\n                {...props}\n              />\n      