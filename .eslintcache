ProjectDetailPage';\nimport ScrollToTop from './ScrollToTop';\nimport { CONSTANTS } from '../data/globals';\n\n// const Projects = lazy(() => import('../components/Project/Projects'));\n// const AboutMe = lazy(() => import('../components/AboutMe/AboutMe'));\n// const Contact = lazy(() => import('../components/Contact/Contact'));\n\n// Sections\n// - Career Goals: Your professional goals for the next five years.\n// - Would be nice\n//    - References - 3-5 people who can verify your professional qualifications, from faculty members, internship supervisors, employment supervisors and supervisors of other activities such as community service projects\n\nconst { STATEMENT } = CONSTANTS;\n\n//Routing tabs\nconst allTabs = ['/', '/projects', '/aboutme', '/contact'];\nconst pageTitles = ['Home', 'Projects', 'About me', 'Contact'];\n\nexport default function App() {\n\n  useEffect(() => {\n    console.log(STATEMENT);\n  }, []);\n\n\n  return (\n    <Theme>\n      <CssBaseline />\n      <BrowserRouter>\n        <div className='App' style={{ minHeight: `calc(100vh - 56px)` }}>\n          <Nav routes={allTabs} pageTitles={pageTitles}>\n            <Route\n              path='/'\n              render={({ location }) => (\n                <>\n                  <NavTabs\n                    routes={allTabs}\n                    value={location.pathname}\n                    pageTitles={pageTitles}\n                  />\n                </>\n              )}\n            />\n          </Nav>\n          <Route\n            render={({ location }) => (\n              <AnimatePresence exitBeforeEnter initial={false}>\n                <Switch key={location.pathname}>\n                  <Route exact path={allTabs[0]} render={() => <Home />} />\n                  <Route\n                    path={`${allTabs[1]}/:id`}\n                    render={() => <ProjectDetailPage />}\n                  />\n                  <Route path={allTabs[1]} render={() => <Projects />} />\n                  <Route path={allTabs[2]} render={() => <AboutMe />} />\n                  <Route path={allTabs[3]} render={() => <Contact />} />\n                </Switch>\n              </AnimatePresence>\n            )}\n          />\n        </div>\n        <Footer />\n        <ScrollToTop />\n      </BrowserRouter>\n    </Theme>\n  );\n}\n","/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/MyPortfolio/src/App/ScrollToTop.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/MyPortfolio/src/components/Nav.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/MyPortfolio/src/components/NavTabs.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/MyPortfolio/src/components/Theme.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/MyPortfolio/src/components/ProjectDetailPage/ProjectDetailPage.jsx",["130"],"import React from 'react';\nimport { Typography, Box, Grid, Container } from '@material-ui/core';\nimport Description from '../Home/Description';\nimport { useParams } from 'react-router-dom';\nimport { PropTypes } from 'prop-types';\nimport data from '../../data/projects.json';\nimport { MetaDataList } from './MetaDataList';\n\n//TODO:\n// pics for projects\nconst { projects } = data;\n\nfunction Title(props) {\n  return (\n    <>\n      <Typography variant='h1'>{props.children}</Typography>\n    </>\n  );\n}\n\nexport function MetaData(props) {\n  return (\n    <Grid container justify={'space-between'}>\n      {props.children}\n    </Grid>\n  );\n}\n\nfunction getProject(targetId) {\n  const result = projects.find(({ id }) => id === targetId);\n  return result;\n}\n\nfunction Heading(props) {\n  return (\n    <Box>\n      <Typography variant={'h2'}>{props.children}</Typography>\n    </Box>\n  );\n}\n\nexport default function ProjectDetailPage(props) {\n  const { id } = useParams(); //pure item id\n  \n  //use the project id to find project data\n  const projectData = getProject(id);\n\n  const {\n    title,\n    content,\n    live,\n    type,\n    techs,\n    repo,\n    illustrations,\n  } = projectData;\n\n  let illustrationPaths = [];\n  if (illustrations) {\n    illustrationPaths = illustrations.map(\n      (imgName) => process.env.PUBLIC_URL + '/img/illustrations/' + imgName\n    ); // local path\n  }\n\n  // render the img by index\n  const renderIllustration = (index) => {\n    try {\n      if (illustrationPaths[index]) {\n        return (\n          <Box\n            maxWidth={1000}\n            maxHeigth={1280}\n            display='flex'\n            justifyContent='center'\n          >\n            <img\n              src={illustrationPaths[index]}\n              alt={'illustration'}\n              style={{ maxWidth: ' 100%', height: 'auto' }}\n            />\n          </Box>\n        );\n      }\n    } catch {\n      return null;\n    }\n  };\n\n  return (\n    <>\n      <Container>\n        <Box mt={14}>\n          <Title>{title}</Title>\n          <Box mt={6} maxWidth={800}>\n            <Description>{content.introduction}</Description>\n          </Box>\n          <Box mt={4} maxWidth={870}>\n            <MetaData>\n              <MetaDataList label={'Type'} item={type} />\n              <MetaDataList label={'Techs'} items={techs} />\n              <MetaDataList\n                label={'Source Code'}\n                item={repo}\n                isLink={true}\n                linktext={'Repo'}\n              />\n              <MetaDataList\n                label={'Demo'}\n                item={live}\n                isLink={true}\n                linktext={'Visit Site'}\n              />\n            </MetaData>\n          </Box>\n        </Box>\n        <Box mt={4}>{renderIllustration(0)}</Box>\n        <Box mt={6} maxWidth={1024}>\n          <Heading>Project Goal</Heading>\n          <Box mt={2}>\n            <Description>{content.goal}</Description>\n          </Box>\n        </Box>\n        {content.spotlight ? (\n          <Box mt={8} maxWidth={1024}>\n            <Heading>Spotlight</Heading>\n            <Box mt={2}><Description>{content.spotlight}</Description></Box>\n          </Box>\n        ) : null}\n        <Box mt={10}>\n          <Grid container spacing={5}>\n            <Grid item xs={12} lg={6}>\n              {renderIllustration(1)}\n            </Grid>\n            <Grid item xs={12} lg={6}>\n              <Heading>Problems</Heading>\n              <Box mt={5} maxWidth={650}>\n                {content.problem.map((p, index) => (\n                  <Description key={'para-' + index}>{p}</Description>\n                ))}\n              </Box>\n            </Grid>\n          </Grid>\n        </Box>\n        <Box mx={'auto'} mt={10} mb={10} maxWidth={1024}>\n          <Typography variant={'h2'} align={'center'}>\n            Lessons Learned\n          </Typography>\n          <Box mt={6}>\n            {content.lesson.map((p, index) => (\n              <Description key={'para-' + index} align={'center'}>\n                {p}\n              </Description>\n            ))}\n          </Box>\n        </Box>\n      </Container>\n    </>\n  );\n}\n","/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/MyPortfolio/src/components/Project/Projects.jsx",["131"],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/MyPortfolio/src/components/Contact/Contact.jsx",["132","133","134"],"// include a contact form and your social media channels.\nimport React, { useState } from 'react';\nimport {\n  Typography,\n  Paper,\n  Box,\n  Button,\n  Grid,\n  TextField,\n  useMediaQuery,\n  Modal,\n  CircularProgress\n} from '@material-ui/core';\n\nimport { makeStyles } from '@material-ui/core/styles';\nimport SocialMedia from './SocialMedia';\nimport { useTheme } from '@material-ui/styles';\nimport { motion } from 'framer-motion';\nimport { useForm } from 'react-hook-form';\n\nconst useStyles = makeStyles((theme) => {\n  const py = theme.spacing(10);\n  return {\n    textArea: {\n      // maxWidth: \"90vw\",\n      width: '100%',\n      fontSize: theme.spacing(2.1),